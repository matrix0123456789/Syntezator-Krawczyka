<UserControl x:Class="Syntezator_Krawczyka.Synteza.cutoffUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:l="clr-namespace:Syntezator_Krawczyka"
             
             mc:Ignorable="d" Background="Chartreuse"
             d:DesignHeight="300" d:DesignWidth="300" MouseMove="UserControl_MouseMove">
    <Grid>
        <l:suwak Height="23"  HorizontalAlignment="Left" Margin="0,22,0,0" x:Name="sliderA" VerticalAlignment="Top" Width="100" SelectionStart="0.5" Maximum="1" ValueChanged="slider1_ValueChanged" Opis="Moc" />
        <l:suwak Height="23"  HorizontalAlignment="Left" Margin="0,50,0,0" x:Name="sliderB" VerticalAlignment="Top" Width="100" SelectionStart="0.5" Maximum="1" ValueChanged="slider2_ValueChanged" Opis="Gładkość" />
        <Label Content="Cutoff" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" />
    </Grid>

</UserControl>
